{
    "manifest_version": 2,

    "name": "Winston",
    "description": "A launcher for the web",
    "version": "0.0.1",

    "browser_action": {
        "default_icon": "src/img/icon.png",
        "default_popup": "src/popup.html"
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["src/js/content.js"]
    }],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+W",
                "mac": "MacCtrl+Shift+W",
                "chromeos": "Ctrl+Shift+W",
                "linux": "Ctrl+Shift+W"
            }
        }
    }
}
